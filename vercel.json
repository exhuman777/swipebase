{
  "rewrites": [
    { "source": "/", "destination": "/swipebase-cloud.html" },
    { "source": "/lite", "destination": "/swipebase.html" },
    { "source": "/docs", "destination": "/swipebase-docs.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
